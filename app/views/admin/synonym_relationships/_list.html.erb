<% @taxon_concept.synonym_relationships.each do |s| %>
  <%= link_to s.other_taxon_concept.full_name, edit_admin_taxon_concept_url(s.other_taxon_concept) %>
  <span class="author-year"><%= s.other_taxon_concept.author_year %></span>
  <%= link_to edit_icon, edit_admin_taxon_concept_synonym_relationship_url(@taxon_concept, s), :remote => true %>
  <% if can? :destroy, s %>
    <%= link_to delete_icon, admin_taxon_concept_synonym_relationship_url(@taxon_concept, s),
      :confirm => "Warning: you are about to delete data. Are you sure?",
      :method => :delete %>
  <% end %>
  <br>
<% end %>
