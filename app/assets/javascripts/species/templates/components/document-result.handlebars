<td class="event-col">{{unbound doc.event_name}}</td>
<td class='event-date-col'>{{unbound doc.date}}</td>
<td class='title-col'>

<a class="legal-links tooltip" {{bind-attr href=view.documentUrl}}>
    {{truncate title}}
    <span>
        {{unbound title}}
    </span>
</a>
</td>
<td class='language-col'>
  {{#if moreThanOneLanguage}}
    {{view Ember.Select content=languages selection=language}}
  {{else}}
    {{language}}
  {{/if}}
</td>
<td class='countries-col'>{{documents-tooltip data=doc.geo_entity_names type='countries'}}</td>
<td class='taxon-col'>{{documents-tooltip data=doc.taxon_names type='taxa'}}</td>
{{#if proposalOutcome}}
  <td>{{unbound doc.proposal_outcome}}</td>
{{/if}}
{{#if reviewPhase}}
  <td>{{unbound doc.review_phase}}</td>
{{/if}}
<td class='download-col'>
  <input type="checkbox">
</td>
