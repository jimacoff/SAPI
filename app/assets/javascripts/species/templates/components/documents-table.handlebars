{{#if documents.length}}
  <table class="documents-table">
    <thead>
      <tr class="documents-head">
        <td>Meeting</td>
        <td>Date</td>
        <td>Type</td>
        <td>Title</rd>
      </tr>
    </thead>
    <tbody>
      {{#each document in documents}}
        <tr class="document-row current">
          <td class="event-name">{{document.event_name}}</td>
          <td class="event-date">{{document.event_date}}</td>
          <td class="event-type">{{document.event_type}}</td>
          <td class="document-title"><a class="legal-links" href="/api/v1/documents/{{unbound document.id}}">{{document.title}}</a></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <span>No data available</span>
{{/if}}
