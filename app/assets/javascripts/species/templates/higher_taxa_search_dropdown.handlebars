<div class="popup">
  <div class="search-block01">
    <div class="row01">
      {{view Species.HigherTaxaSearchTextField
        valueBinding="controller.taxonConceptQuery"
        placeholder="Type to filter higher taxa"
        autocomplete="off"
      }}
      <input type="submit" value="submit" />
    </div>
    <strong class="title01">SELECTED TAXA</strong>
    {{collection Species.SelectedHigherTaxaCollectionView
      contentBinding="controller.selectedTaxonConcepts"}}
  </div>
  <div class="list-holder">
    <ul>
      <li>
        {{collection Species.HigherTaxaCollectionView
          contentBinding="controller.autoCompleteTaxonConcepts"}}
      </li>
    </ul>
  </div>
</div>
