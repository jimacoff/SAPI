<div class="row-fluid">
  <div class="span2">
    {{#if isPrimary}}
      <span class="label label-important">PRIMARY</span>
    {{else}}
      <span class="label label-warning">SECONDARY</span>
    {{/if}}
  </div>
  <div class="span10">
    <a {{action setFiltersFromErrorSelector errorSelector}}>
      {{errorMessage}} ({{errorCount}})
    </a>
    <a {{action setVisibleShipments this.sandboxShipments }}>
      (old buggy link)
    </a>

  </div>
</div>
