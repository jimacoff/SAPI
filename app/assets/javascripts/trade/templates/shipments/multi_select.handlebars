{{view Trade.MultiSelectButton
  selectedValuesBinding="view.selectedValues"
  selectedValuesCollectionNameBinding="view.selectedValuesCollectionName"
  selectedValueDisplayPropertyBinding="view.selectedValueDisplayProperty"
}}
{{view Trade.MultiSelectDropdown
  queryBinding="view.query"
  allValuesBinding="view.allValues"
  allValuesGroupedBinding="view.allValuesGrouped"
  selectedValuesBinding="view.selectedValues"
  selectedValueDisplayPropertyBinding="view.selectedValueDisplayProperty"
  selectedValueShortDisplayPropertyBinding="view.selectedValueShortDisplayProperty"
}}
